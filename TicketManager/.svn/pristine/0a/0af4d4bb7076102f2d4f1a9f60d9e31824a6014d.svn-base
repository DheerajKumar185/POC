<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TicketManager</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>CheckUser1</servlet-name>
    <servlet-class>CheckUser1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckUser1</servlet-name>
    <url-pattern>/CheckUser1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChangePwd</servlet-name>
    <servlet-class>ChangePwd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChangePwd</servlet-name>
    <url-pattern>/ChangePwd</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>CheckUser2</servlet-name>
  	<servlet-class>CheckUser2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CheckUser2</servlet-name>
  	<url-pattern>/CheckUser2</url-pattern>
  </servlet-mapping>
</web-app>