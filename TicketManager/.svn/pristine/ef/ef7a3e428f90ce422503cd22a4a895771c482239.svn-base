</head>
<body>
<div id="Layer1">
  <div>
    <form id="form1" name="form1" method="post" action="CheckUser">
      <table width="100%" align="center">
        <tr>
          <td width="36%">User Name </td>
          <td width="14%">:</td>
          <td width="50%"><label for="textfield"></label>
          <input type="text" name="userName" tabindex="1" id="textfield" /></td>
        </tr>
        <tr>
          <td>Password</td>
          <td>:</td>
          <td><label for="label"></label>
          <input type="password" name="password" tabindex="2" id="label" /></td>
        </tr>
      </table>
	  <br />
	  <div>
	    <table width="100%">
          <tr>
            <td width="19%">&nbsp;</td>
            <td width="17%"><label for="Submit"></label>
            <input type="submit" name="Submit" value="Login" tabindex="3" id="Submit" /></td>
            <td width="20%">&nbsp;</td>
            <td width="26%"><label for="label2"></label>
            <input type="reset" name="Submit2" value="Reset" tabindex="4" id="label2" /></td>
            <td width="18%">&nbsp;</td>
          </tr>
        </table>
	  </div>
    </form>
  </div>
  <br />  
</div>
</body>

